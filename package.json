{
  "name": "@kasplex/kiwi",
  "version": "1.0.4",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "A powered solution for effortless Kaspa wallet integration, delivering secure and streamlined blockchain operations.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/kiwi.js"
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "test": "vitest",
    "start": "bun run src/index.ts",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "bun build ./src/index.ts --outdir ./dist --entry-naming [dir]/[name].[ext] --asset-naming [name].[ext] --target=node",
    "prepublishOnly": "bun run build && bun run build:types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kasplex/sdk-kiwi.git"
  },
  "keywords": [
    "kasplex sdk",
    "kaspa wasm",
    "krc20 sdk",
    "kaspa wallet",
    "kiwi",
    "kasplex"
  ],
  "author": "kasplex",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "bun-types": "^1.2.4",
    "jest": "^29.7.0",
    "terser": "^5.24.4",
    "ts-jest": "^29.2.6",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.0.0",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "buffer": "^6.0.3",
    "js-sha3": "^0.8.0",
    "node-fetch": "^3.3.2"
  },
  "type": "module"
}
