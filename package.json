{
  "name": "@kasplex/kiwi",
  "version": "1.0.1",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "A powered solution for effortless Kaspa wallet integration, delivering secure and streamlined blockchain operations.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/kiwi.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "bun test",
    "start": "bun run src/index.ts",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "bun build ./src/index.ts --outdir ./dist --entry-naming [dir]/[name].[ext] --asset-naming [name].[ext]",
    "prepublishOnly": "bun run build && bun run build:types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kasplex/sdk-kiwi.git"
  },
  "keywords": ["kaspa", "sdk", "wallet", "blockchain", "kiwi", "sdk-kiwi", "kiwi"],
  "author": "kasplex",
  "license": "ISC",

  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "bun-types": "^1.2.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.0.0"
  },
  "dependencies": {
    "@kasplex/kiwi": "^1.0.1",
    "@noble/hashes": "^1.7.1",
    "@noble/secp256k1": "^2.2.3",
    "bech32": "^2.0.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "elliptic": "^6.6.1",
    "ethereum-cryptography": "^3.1.0",
    "js-sha3": "^0.8.0"
  },
  "type": "module"
}
